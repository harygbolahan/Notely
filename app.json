{
  "expo": {
    "name": "Notely",
    "slug": "notely",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Notely.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Notely.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.notely.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Notely.png",
        "backgroundColor": "#3B82F6"
      },
      "package": "com.notely.app",
      "permissions": [],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
